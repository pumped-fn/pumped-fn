{
  "name": "@pumped-fn/devtools",
  "version": "0.2.15",
  "description": "Development tools for pumped-fn applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "pumped-cli": "./dist/bin/pumped-cli.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "typecheck:full": "tsc --noEmit --project tsconfig.test.json"
  },
  "dependencies": {
    "@pumped-fn/core-next": "workspace:*",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/ws": "^8.5.13",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.2",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
