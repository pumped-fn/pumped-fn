{
  "name": "@pumped-fn/lite",
  "version": "0.0.1",
  "description": "Lightweight dependency injection with minimal reactivity",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc --noEmit",
    "typecheck:full": "tsc --noEmit -p tsconfig.test.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "tsdown": "^0.12.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "peerDependencies": {},
  "keywords": ["dependency-injection", "di", "lite", "reactivity", "typescript"],
  "license": "MIT"
}
